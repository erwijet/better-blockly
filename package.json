{
  "name": "better-blockly",
  "version": "0.3.0",
  "description": "",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./lib": {
      "import": {
        "types": "./dist/lib.d.mts",
        "default": "./dist/lib.mjs"
      },
      "require": {
        "types": "./dist/lib.d.ts",
        "default": "./dist/lib.js"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts src/lib.ts --format cjs,esm --dts"
  },
  "keywords": [
    "blockly"
  ],
  "author": "Tyler Holewinski <tyler@holewinski.dev>",
  "repository": {},
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@tsly/arr": "^1.4.0",
    "@tsly/maybe": "^1.2.0"
  },
  "peerDependencies": {
    "blockly": "^11.0.0"
  }
}
